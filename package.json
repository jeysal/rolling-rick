{
  "name": "rolling-rick",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "type": "module",
  "engines": {
    "node": ">= 18.5.0",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.game.json && tsc --project tsconfig.server.json",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:game\"",
    "dev:game": "tsc --watch --project tsconfig.game.json",
    "dev:server": "tsc-watch --project tsconfig.server.json --onSuccess \"node dist/server.js\"",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playt-net/rolling-rick.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/playt-net/rolling-rick/issues"
  },
  "homepage": "https://github.com/playt-net/rolling-rick#readme",
  "dependencies": {
    "@playt/client": "3.5.0",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "node-fetch": "3.2.6",
    "phaser": "3.55.2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node-fetch": "2.6.2",
    "concurrently": "7.2.2",
    "tsc-watch": "5.0.3",
    "typescript": "4.8.0-dev.20220711"
  }
}
